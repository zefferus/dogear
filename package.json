{
	"name": "dogear",
	"description": "A hapi plugin for sending expanded metrics to statsd-compliant services",
	"version": "2.0.5",
	"engines": {
		"node": ">=4.0.0"
	},
	"main": "src/index.js",
	"scripts": {
		"lint": "eslint src",
		"test": "tap --cov --reporter=spec test/*.test.js",
		"precoverage": "rimraf coverage",

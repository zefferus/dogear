	"author": "Brandon Belvin",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/zefferus/dogear/issues"
	},
	"homepage": "https://github.com/zefferus/dogear#readme",
	"dependencies": {
		"hot-shots": "^5.5.1",
		"hoek": "^4.0.2",
		"oppsy": "^1.0.2",
		"lodash.at": "^4.5.0"
	},
	"devDependencies": {
		"hapi": "^14.0.0",
		"babel-eslint": "^6.1.2",
		"eslint": "^3.1.1",
